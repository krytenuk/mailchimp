        "laminas/laminas-stdlib": "^3.6",
        "laminas/laminas-http": "^2.15",
        "laminas/laminas-hydrator": "^4.3",
        "laminas/laminas-json": "^3.3",
        "laminas/laminas-servicemanager": "^3.10"
    },
    "authors": [{
